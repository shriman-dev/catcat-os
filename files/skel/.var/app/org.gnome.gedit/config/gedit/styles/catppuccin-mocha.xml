<style-scheme name="Catppuccin Mocha" id="catppuccin_mocha" version="1.0">
  <author>sacerdos</author>
  <description>Soothing pastel theme for Gedit</description>

  <style name="text"	foreground="#cdd6f4" background="#1e1e2e" bold="true" />
  <style name="selection"	foreground="#181825" background="#fab387" />
  <style name="cursor"	foreground="#f5e0dc" />
  <style name="secondary-cursor"	foreground="#f5e0dc" />
  <style name="current-line"	background="#373754" />
  <style name="line-numbers"	foreground="#cdd6f4" background="#11111b" />
  <style name="draw-spaces"	foreground="#cdd6f4" />
  <style name="background-pattern"	background="#181825" />
  <style name="bracket-match"	foreground="#ffe8b5" />
  <style name="bracket-mismatch"	foreground="#cdd6f4" background="#fab387" />
  <style name="right-margin"	foreground="#cdd6f4" background="#11111b" />
  <style name="search-match"	foreground="#181825" background="#89b4fa" />
  <style name="def:comment"	foreground="#6c7086" />
  <style name="def:shebang"	foreground="#6c7086" bold="true" />
  <style name="def:doc-comment-element"	italic="true" />
  <style name="def:constant"	foreground="#a6e3a1" />
  <style name="def:string"	foreground="#a6e3a1" />
  <style name="def:special-char"	foreground="#b4befe" />
  <style name="def:special-constant"	foreground="#b4befe" />
  <style name="def:floating-point"	foreground="#b4befe" />
  <style name="def:identifier"	foreground="#89b4fa" />
  <style name="def:statement"	foreground="#74c7ec" bold="true" />
  <style name="def:type"	foreground="#eba0ac" bold="true" />
  <style name="def:emphasis"	italic="true" />
  <style name="def:strong-emphasis"	foreground="#f9e2af" bold="true" />
  <style name="def:inline-code"	foreground="#a6e3a1" />
  <style name="def:insertion"	underline="true" />
  <style name="def:deletion"	strikethrough="true" />
  <style name="def:link-text"	foreground="#f5e0dc" />
  <style name="def:link-symbol"	foreground="#89b4fa" bold="true" />
  <style name="def:link-destination"	foreground="#89b4fa" italic="true" underline="true" />
  <style name="def:heading"	foreground="#94e2d5" bold="true" />
  <style name="def:thematic-break"	foreground="#a6e3a1" bold="true" />
  <style name="def:preformatted-section"	foreground="#a6e3a1" />
  <style name="def:list-marker"	foreground="#94e2d5" bold="true" />
  <style name="def:preprocessor"	foreground="#94e2d5" />
  <style name="def:error"	foreground="#eba0ac" bold="true" />
  <style name="def:warning"	foreground="#fab387" />
  <style name="def:note"	foreground="#89b4fa" bold="true" />
  <style name="def:net-address"	italic="true" underline="true" />
</style-scheme>
